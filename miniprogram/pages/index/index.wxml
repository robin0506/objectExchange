<view class="container">
    <view class="objects">

        <view class="object  {{'object-'+index}}" wx:for="{{list}}" wx:key="pic" >
            <view class="object-pic">
                <image wx:if="{{item.show}}" bindtap="previewImage" data-index='{{index}}' src="{{item.pic}}" class="object-picture" mode="aspectFit" alt=""/>
            </view>
            <view class="details">
                <view class="title" >{{item.title}}</view>
            </view>
        </view>


